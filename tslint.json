{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "no-namespace": [
      true,
      "allow-declarations"
    ],
    "no-console": [
      false
    ],
    "max-classes-per-file": false,
    "semicolon": [
      true,
      "always",
      "strict-bound-class-methods"
    ],
    "strict-boolean-expressions": true
  },
  "rulesDirectory": []
}
